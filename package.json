{
  "name": "sockz",
  "version": "3.0.0",
  "description": "Fun with sockets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sockz": "bin/sockz",
    "sockz-creds": "bin/sockz-creds",
    "sockz-agent": "bin/sockz-agent",
    "sockz-server": "bin/sockz-server"
  },
  "scripts": {
    "build": "tsc",
    "pem": "ts-node src/bin/pem.ts",
    "server": "nodemon src/bin/server.ts",
    "agent": "nodemon -d 2 src/bin/agent.ts",
    "ci:audit": "npm audit",
    "ci:install": "npm install",
    "ci:build": "npm run build",
    "ci:setup": "npm run ci:install && npm run ci:lint && npm run ci:build",
    "ci:test": "npm test",
    "ci:lint": "npm run lint",
    "preci": "npm run ci:setup",
    "ci": "npm run ci:test",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "doc": "typedoc src",
    "doc:commit": "npm run doc && git add docs && (git commit -m 'Generated Docs' || echo 'Docs unchanged')",
    "preversion": "npm run ci && npm run doc:commit",
    "postversion": "npm publish && git push --follow-tags",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "keywords": [],
  "author": "CMR1",
  "license": "MIT",
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.1",
    "pem": "^1.14.6",
    "shelljs": "^0.8.5",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@bowtie/prettier": "^1.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.21",
    "@types/pem": "^1.9.6",
    "@types/ws": "^8.5.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-bowtie": "^2.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "terser": "^5.12.0",
    "ts-node": "^10.6.0",
    "typedoc": "^0.22.12",
    "typescript": "4.5"
  }
